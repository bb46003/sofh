<section class="sofh-custom">
  <section>
    <div class="custom-house-header">
      <h3>{{localize "sofh.SETTINGS.add-custom-house-title"}} <a><i class="fa fa-plus" data-action="addHouse"></i></a></h3>
      
    </div>
  </section>

  <section class="custom-houses">
    {{#each houses}}
      {{> "systems/SofH/templates/app/part/custom-house.hbs" house=this}}
    {{/each}}
  </section>

  <section class="custom-blood">

      {{> "systems/SofH/templates/app/part/custom-blood-type.hbs" blood=this}}
 
  </section>
</section>

<footer class="form-footer" style="width: 100%;">
  <button type="submit" data-action="save" autofocus=""style="width: 100%;">
    <i class="fa fa-check"></i>
    <span>{{localize "sofh.SETTINGS.save"}}</span>
  </button>
</footer>
